<h1>Crear Usuario</h1>
<form action="/Saludo/crearUsuarioQuemado" method="get">
  <div class="form-group row">
    <label for="nombres" class="col-2 col-form-label">Nombres</label>
    <div class="col-10">
      <input class="form-control" type="text" value="" placeholder="Ingrese sus nombres" id="nombres" name="nombre">
    </div>
  </div>
  <div class="form-group row">
    <label for="apellidos" class="col-2 col-form-label">Apellidos</label>
    <div class="col-10">
      <input class="form-control" type="text" value="" placeholder="Ingrese sus apellidos" id="apellidos" name="apellidos">
    </div>
  </div>
  <div class="form-group row">
    <label for="correo" class="col-2 col-form-label">Email</label>
    <div class="col-10">
      <input class="form-control" type="email" value="" placeholder="Ingrese su correo" id="correo" name="emailAddress">
    </div>
  </div>
  <div class="form-group row">
    <label for="fecha-nacimiento" class="col-2 col-form-label">Fecha</label>
    <div class="col-10">
      <input class="form-control" type="date" value="" placeholder="dd/mm/yyyy" id="fecha-nacimiento" name="fechaNacimiento">
    </div>
  </div>
  <div class="form-group row">
    <label for="password" class="col-2 col-form-label">Password</label>
    <div class="col-10">
      <input class="form-control" type="password" value="" placeholder="Ingrese su password" id="password" name="password">
    </div>
  </div>

  <button type="button" class="btn btn-outline-secondary">Button</button>
  <button type="submit" class="btn btn-outline-success">Crear</button>
  <button type="reset" class="btn btn-outline-warning">Reset</button>
</form>
